<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabViewList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="10dp"
        android:layout_gravity="bottom|start"
        android:layout_marginEnd="160dp"
        android:clickable="true"
        android:elevation="16dp"
        android:focusable="true"
        android:visibility="invisible"
        app:backgroundTint="@color/newColorElement"
        android:tint="@color/newColorBg"
        app:pressedTranslationZ="12dp"
        app:maxImageSize="25dp"
        android:contentDescription="@string/show_list_timer"
        app:srcCompat="@drawable/list_24" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabViewSave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="90dp"
        android:layout_gravity="bottom|start"
        android:clickable="true"
        android:elevation="16dp"
        android:focusable="true"
        android:visibility="invisible"
        app:maxImageSize="25dp"
        app:backgroundTint="@color/newColorElement"
        android:tint="@color/newColorBg"
        app:pressedTranslationZ="12dp"
        android:contentDescription="@string/saveTimer"
        app:srcCompat="@drawable/save_24" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabAddList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="10dp"
        android:layout_gravity="bottom|start"
        android:tint="@color/newColorBg"
        app:backgroundTint="@color/newColorElement"
        app:maxImageSize="35dp"
        android:clickable="true"
        android:elevation="16dp"
        android:focusable="true"
        android:visibility="visible"
        app:pressedTranslationZ="12dp"
        android:contentDescription="@string/other_function"
        app:srcCompat="@drawable/add_icon" />
</RelativeLayout>